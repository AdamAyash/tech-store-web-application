@model User

@{
    Layout = "";
}

<!DOCTYPE html>
<html lang="bg">
<head>
	<meta name="viewport" content="width=device-width" />
	<script src="~lib/jquery/jquery.js"></script>
	<script src="~lib/bootstrap/js/bootsrap.js"></script>
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
	<link href="~/css/site.css" rel="stylesheet">
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
	<title>@ViewBag.Title</title>
</head>
<body>
	<div class="container">
		<form asp-action="LogIn" method="post" role="form">
			<div class="form-group">
				<label asp-for="Username" asp-for="Username">Username</label>
				<input asp-for="Username" asp-for="Username" class="form-control" id="exampleInputEmail1">
			</div>
			<div class="form-group">
				<label asp-for="Password" asp-for="Password">Password</label>
				<input asp-for="Password" asp-for="Password" class="form-control" id="exampleInputEmail1">
			</div>
			<button type="submit" class="btn btn-primary">Submit</button>
		</form>
	</div>
</body>
</html>

